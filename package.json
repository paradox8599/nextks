{
  "name": "nextks",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "postinstall": "git config core.hooksPath .git-hooks",
    "notrack": "next telemetry disable",
    "env": "dotenvx run --convention=nextjs --",
    "init": "bun run env -- keystone postinstall --fix",
    "dev:ks": "bun run env -- keystone dev --no-db-push",
    "dev:kp": "bun run env -- keystone dev",
    "dev:nx": "next dev -p 4000",
    "dev": "conc -n ks,nx 'bun dev:ks' 'bun dev:nx'",
    "build:ks": "keystone build",
    "build:nx": "next build",
    "start:ks": "keystone start",
    "start:nx": "next start -p 4000",
    "start": "conc -n keystone,next 'bun start:ks' 'bun start:nx'",
    "build": "keystone postinstall --fix && next build && keystone build",
    "lint": "lint-staged",
    "lint:nx": "next lint",
    "migrate": "bun run env -- keystone prisma migrate",
    "test": "jest --coverage --runInBand"
  },
  "dependencies": {
    "@hapi/iron": "^7.0.1",
    "@keystone-6/auth": "^8.0.0",
    "@keystone-6/core": "^6.3.0",
    "@keystone-6/document-renderer": "^1.1.2",
    "@keystone-6/fields-document": "^9.1.0",
    "@ngneat/falso": "^7.2.0",
    "@prisma/client": "5.19.0",
    "concurrently": "^9.0.1",
    "dayjs": "^1.11.13",
    "graphql": "^16.9.0",
    "graphql-request": "^7.1.0",
    "graphql-yoga": "^3.9.1",
    "lodash": "^4.17.21",
    "next": "14.2.11",
    "react": "^18",
    "react-dom": "^18",
    "swr": "^2.2.5",
    "valtio": "^2.1.0"
  },
  "devDependencies": {
    "@dotenvx/dotenvx": "^1.20.1",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.2",
    "@testing-library/react": "^16.0.1",
    "@types/bun": "latest",
    "@types/lodash": "^4.17.12",
    "@types/node": "^20.17.0",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "aws-sdk": "^2.1691.0",
    "eslint": "^8.57.1",
    "eslint-config-next": "14.2.11",
    "eslint-plugin-valtio": "^0.6.4",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lint-staged": "^15.2.10",
    "postcss": "^8.4.47",
    "prettier": "^3.3.3",
    "tailwindcss": "^3.4.14",
    "typescript": "^5.6.3"
  },
  "lint-staged": {
    "*.{tsx,jsx,ts,js}": [
      "eslint --fix",
      "prettier --write"
    ]
  }
}
